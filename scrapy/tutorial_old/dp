<script type="text/javascript">
 
P.when('DynamicIframe').execute(function(DynamicIframe){
	var BookDescriptionIframe = null,
	    bookDescEncodedData = "%3CP%20style%3D%22MARGIN%3A%200px%22%3E%20%3CB%3EPraise%20for%20%3CI%3ECore%20Python%20Programming%3C%2FI%3E%20%3C%2FB%3E%20%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%26nbsp%3B%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%22The%20long-awaited%20second%20edition%20of%20Wesley%20Chun%26%2339%3Bs%20%3CI%3ECore%20Python%20Programming%3C%2FI%3E%20proves%20to%20be%20well%20worth%20the%20wait%26mdash%3Bits%20deep%20and%20broad%20coverage%20and%20useful%20exercises%20will%20help%20readers%20learn%20and%20practice%20good%20Python.%22%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%26mdash%3BAlex%20Martelli%2C%20author%20of%20%3CI%3EPython%20in%20a%20Nutshell%3C%2FI%3E%20and%20editor%20of%20%3CI%3EPython%20Cookbook%3C%2FI%3E%20%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%26nbsp%3B%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%22There%20has%20been%20lot%20of%20good%20buzz%20around%20Wesley%20Chun%26%2339%3Bs%20%3CI%3ECore%20Python%20Programming%3C%2FI%3E.%20It%20turns%20out%20that%20all%20the%20buzz%20is%20well%20earned.%20I%20think%20this%20is%20the%20best%20book%20currently%20available%20for%20learning%20Python.%20I%20would%20recommend%20Chun%26%2339%3Bs%20book%20over%20%3CI%3ELearning%20Python%3C%2FI%3E%20%28O%26%2339%3BReilly%29%2C%20%3CI%3EProgramming%20Python%3C%2FI%3E%20%28O%26%2339%3BReilly%29%2C%20or%20%3CI%3EThe%20Quick%20Python%20Book%3C%2FI%3E%20%28Manning%29.%22%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%26mdash%3BDavid%20Mertz%2C%20Ph.D.%2C%20IBM%20DeveloperWorks%26reg%3B%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%26nbsp%3B%3C%2FP%3E%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%22I%20have%20been%20doing%20a%20lot%20of%20research%20%5Bon%5D%20Python%20for%20the%20past%20year%20and%20have%20seen%20a%20number%20of%20positive%20reviews%20of%20your%20book.%20The%20sentiment%20expressed%20confirms%20the%20opinion%20that%20%3CI%3ECore%20Python%20Programming%3C%2FI%3E%20is%20now%20considered%20